{
  "name": "tweet-search-cli",
  "version": "1.0.0",
  "description": "A tweet search CLI application built with Ink and TypeScript",
  "type": "module",
  "bin": {
    "tweet-search": "./src/index.tsx"
  },
  "scripts": {
    "start": "tsx src/index.tsx",
    "dev": "tsx watch src/index.tsx",
