    "react": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^24.0.7",
    "@types/react": "^18.3.3",
    "lefthook": "^1.11.16",
    "tsx": "^4.19.2",
    "typescript": "^5.5.3"
  }
}
